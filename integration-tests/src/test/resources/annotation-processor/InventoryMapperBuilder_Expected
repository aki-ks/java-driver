package com.datastax.oss.driver.mapper.model.inventory;

import com.datastax.oss.driver.api.core.session.Session;
import com.datastax.oss.driver.internal.mapper.MapperContext;

/**
 * Builds an instance of {@link InventoryMapper} wrapping a driver {@link Session}.
 *
 * <p>Generated by the DataStax driver mapper, do not edit directly.
 */
public class InventoryMapperBuilder {
  private final Session session;

  public InventoryMapperBuilder(Session session) {
    this.session = session;
  }

  public InventoryMapper build() {
    MapperContext context = new MapperContext(session);
    return new InventoryMapper_Impl(context);
  }
}
